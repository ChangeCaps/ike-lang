import std::string
import std::io
import std::list

fn main {
  let file = file::new "ike/parse/tokenize.ike"
  let ts, diagnostics = parse::tokenize file 
    <| (file |> file::read |> std::result::assert)

  diagnostics
    |> list::map diagnostic::format
    |> list::foldl "" string::append
    |> io::print

  std::debug::print ts
}
